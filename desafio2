#include <iostream>
#include <iomanip>

using namespace std; 

int main() {

    int x, y, z;

    cout << "Digite a primeira altura descoberta: " << endl;
    cin >> x;
   
    cout << "Digite a segunda altura descoberta: " << endl;
    cin >> y;
 
    cout << "Digite a terceira altura descoberta: " << endl;
    cin >> z;

    if (x>y>z) {

        cout << "Altura do Gatti é: " << y;
    
    } else if (y>x>z){

        cout << "Altura do Gatti é: " << x;

    } else if (y>z>x){

        cout << "Altura do Gatti é: " << z;
        
    }

    

}